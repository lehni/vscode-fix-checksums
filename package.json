      {
        "command": "fixChecksums.restore",
        "title": "Fix Checksums: Restore"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42",
    "eslint": "^4.11.0",
    "vscode": "^1.1.6"
  }
